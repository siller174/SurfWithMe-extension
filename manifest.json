{
  "manifest_version": 2,
  "name": "Surf with me",
  "short_name": "SurfWithMe",
  "version": "0.1.0",
  "description": "Share special code and surf with friends",
  "permissions": ["storage", "tabs", "activeTab", "<all_urls>"],
  "browser_action": {
    "default_popup": "index.html",
    "default_action": "logo.png"
  },
  "background": {
    "scripts": [
      "background.ts"
    ]
  }
}
